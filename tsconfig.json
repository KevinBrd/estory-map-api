{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "lib": [
        "ES6"
      ],
      "outDir": "./dist",
      "rootDir": "./src",
      "sourceMap": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "strict": true,
      "types": [
        "node",
      ]
    },
    "exclude": [
      "node_modules",
      "tests"
    ]
}